{% extends 'Common/_entity-widget.html.twig' %}

{% set entity = wood %}

{% block heading %}
    {% if wood.family == 1 %}
        <i class="ladb-icon-hardwood ladb-icon-lg"></i>
    {% elseif wood.family == 2 %}
        <i class="ladb-icon-resinous ladb-icon-lg"></i>
    {% endif %}
    {{ parent() }}
{% endblock %}

{% block subheading %}
    {{ wood.scientificname }}
{% endblock %}